# DataFrame 정렬
df = pd.DataFrame( [[ 4, 5, 1, 2],
                    [ 300, 400, 200, 100],
                    [ 20, 10, 40, 30 ]], index=[ 'c', 'a', 'b' ], columns=[ 'D', 'C', 'A', 'B' ])
