# iloc
df = pd.DataFrame( np.arange( 1, 21 ). reshape( 5, 4), columns=['A', 'B', 'C', 'D'] )
print( df )

# print( df[2][2] )              #에러
print( df['C'][2])
print( df[['A', 'D']] )
# print( df[['A' : 'C']] )        #에러
# print( df[[ 'A' : 'C' ]] )        #에러

print( df.iloc[2][2] )
print( df.iloc[2, 2] )
#print( df.iloc[2, 'C'] )          #에러
print( df.iloc[:2, :2] )
print( df.iloc[:3:2, :3:2] )
print( df.iloc[:2, :-1] )
print( df.iloc[::-1, ::-1] )


# Loc
print( df )
#print( df.lod[2, 2] )                      #에러
print( df.loc[2, 'C'] )
#print( df.loc[:2, :2] )                     #에러
print( df.loc[:2, :'C'] )
print( df[:2] )
print( df.loc[1:3, 'B':'D'] )
print( df.loc[:, 'C'] )
print( df.loc[:, ['B', 'D']] )
print( df.loc[:, ['B', 'D']] )
print( df[['B', 'D']] )


df = pd.DataFrame( np.arange( 1, 21 ).reshape( 5, 4 ), columns=['A', 'B', 'C', 'D'] )
df['E'] = [ 31, 32, 33, 34, 35 ]
# df[5][:] = [90, 91, 92, 93, 94 ]
# df.iloc[:, 5] = [ 40, 41, 42, 43, 44 ]             #에러
# df.iloc[5, :] = [ 90, 92, 92, 93, 94 ]               #에러
df.loc[5] = [ 90, 91, 92, 93, 94 ]
df.loc[:, 'F'] = [ 40, 41, 42, 43, 44, 45 ]

df.loc[0, 'A'] = 99
