def add_many(*args):
    result = 0
    for i in args:
        result = result+i
    return result


add_many(1, 2, 3, 4, 5, 6)





def add_mul(option, *args):
    if option == 'mul':
        data = 1
        for i in args:
            data = data * i
        return data

    if option == 'sum':
        data = 0
        for i in args:
            data = data + i
        return data


add_mul('mul', 1,2,3,4,5)


add_mul('sum', 1,2,3,4,5)





